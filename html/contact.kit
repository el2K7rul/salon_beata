<!DOCTYPE html>
<html lang="en">
  <!--@include 'head.kit'-->

  <body>
    <!--@include 'nav.kit'-->
    <section class="contact">
      <div class="contact__text">
        <h1 class="contact__text--heading">Kontakt</h1>
        <h2 class="contact__text--subheading">Umów się na wizytę!</h2>
      </div>
      <div class="contact__box">
        <div class="contact__tel">
          <a href="tel:+48501293984" class="contact__tel-link"><i class="fa-solid fa-phone"></i> 501 293 984</a>
        </div>

        <div class="contact__mail">
          <a href="mailto: beata@salonbeata.pl" class="contact__mail-link"><i class="fa-solid fa-envelope"></i> beata@salonbeata.pl</a>
        </div>
        <div class="contact__adress">
            <p class="contact__adress-text"><i class="fa-solid fa-location-dot"></i> Strażacka 1, 19-100 Mońki</p>
        </div>

        <div class="contact__socials">
          <a href="https://www.facebook.com/beata.nieciecka.9" target="_blank"
          class="contact__socials-link"><i class="fa-brands fa-facebook contact__socials--ico"> </i
        ></a>
        </div>

      </div>
      

      <form action="#" class="form">
        <div class="form__box">
          <label for="name" class="form__label">Imię:</label>
          <input type="text" id="name" name="tel" class="form__input">
        </div>

        <div class="form__box">
          <label for="tel" class="form__label">Telefon:</label>
          <input type="tel" id="tel" name="tel" class="form__input">
        </div>

        <dic class="form__box">
          <label for="mail" class="form__label">e-mail:</label>
          <input type="email" id="mail" name="mail" class="form__input">
        </dic>

        <div class="form__box">
          <label for="msg" class="form__label">Twoja wiadomość:</label>
          <textarea name="message" id="msg" class="form__msg"></textarea>
        </div>

        <div class="button-box"><button class="form__btn">Wyślij</button></div>
      </form>
    </section>
    <section class="location">
      <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d9514.885278179383!2d22.790207873348923!3d53.40192485688435!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46e025769d5d69ff%3A0x5c914c56cfbbc88e!2sSalon%20fryzjerski%20Beata!5e0!3m2!1spl!2spl!4v1725452755517!5m2!1spl!2spl" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    </section>


    <!--@include 'footer.kit'-->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="./dist/js/main.min.js"></script>
  </body>
</html>
</html>


